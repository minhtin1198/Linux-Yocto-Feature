        color: rgb(128,128,128);
      }

      h6.h2{
        color: rgb(0,0,0);
        font-style: normal;
        text-decoration: underline;
      }

      h6.h3{
        color: rgb(128,128,128);
        font-style: normal;
      }

      h6.h4{
        color: rgb(0,0,0);
        text-decoration: underline;
      }
      .form-id{
        white-space: nowrap;
      }
      .field-value{
        margin-left: 1em;
      }
      .field-value td{
        padding: 0px 2px 0px 0px;
      }

      .positive{
        color: rgb(0,128,64) !important;
      }
      .block-positive{
      }

      .neutral{
