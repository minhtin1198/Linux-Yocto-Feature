        font-size: 14px;
        color: rgb(0,0,0);
        font-style: italic;
      }

      h6.h1{
        color: rgb(128,128,128);
      }

      h6.h2{
        color: rgb(0,0,0);
        font-style: normal;
        text-decoration: underline;
      }

      h6.h3{
        color: rgb(128,128,128);
        font-style: normal;
      }

      h6.h4{
        color: rgb(0,0,0);
        text-decoration: underline;
      }
      .form-id{
        white-space: nowrap;
      }
      .field-value{
        margin-left: 1em;
      }
      .field-value td{
        padding: 0px 2px 0px 0px;
      }

      .positive{
        color: rgb(0,128,64) !important;
      }
      .block-positive{
      }

      .neutral{
        color: rgb(128,128,128) !important;
      }
      .block-neutral{
      }

      .negative{
        color: rgb(204,0,0) !important;
      }
      .block-negative{
      }

      .highlight{
        color: rgb(0,0,0) !important;
        background-color: rgb(240,204,204) !important;
      }
      .block-highlight{
      }
      .nr{
      }
      .block-nr{
      }
